{
    "hosting": {
        "public": "frontend",
        "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
        "headers": [
            {
                "source": "**",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "max-age=300"
                    }
                ]
            }
        ]
    }
}
